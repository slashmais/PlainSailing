<div style="margin-left: 50px; ">
	<h2>Log In</h2>
	<p>
		<%= form_with url: sign_in_path do |f| %>
			<table>
				<tr><td align="center"><%= f.label 'email' %></td><td><%= f.text_field :email %></td></tr>
				<tr><td align="center"><%= f.label 'password' %></td><td><%= f.password_field :password, id: 'password' %></td></tr>
				<tr><td></td><td><%= f.submit 'Log In' %></td></tr>
			</table>
		<% end %>
	</p>
</div>
